		$j = jQuery.noConflict();
		$j(document).ready(function(){
			
			now = new Date();
			then = new Date("Aug 13 2012 23:59:59");
			
			days = (then - now) / 1000 / 60 / 60 / 24;
			daysRound = Math.floor(days);
			if( daysRound < 10 ) daysRound = "0" + daysRound
			
			hours = (then - now) / 1000 / 60 / 60 - (24 * daysRound);
			hoursRound = Math.floor(hours);
			if( hoursRound < 10 ) hoursRound = "0" + hoursRound
			
			minutes = (then - now) / 1000 /60 - (24 * 60 * daysRound) - (60 * hoursRound);
			minutesRound = Math.floor(minutes);
			if( minutesRound < 10 ) minutesRound = "0" + minutesRound
			
			seconds = (then - now) / 1000 - (24 * 60 * 60 * daysRound) - (60 * 60 * hoursRound) - (60 * minutesRound);
			secondsRound = Math.round(seconds);
			if( secondsRound < 10 ) secondsRound = "0" + secondsRound
			
			$j('#counter').countdown({
				image: 'images/digits.png',
				startTime: daysRound + ':' + hoursRound + ':' + minutesRound + ':' + secondsRound
			});
					   
		});
